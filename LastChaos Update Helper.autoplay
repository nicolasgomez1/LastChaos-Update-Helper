<Event>
<Name>On Click</Name>
<Args/>
<Script>if(Grid.GetRowCount("Grid1") &gt; 0)then
	local TempFolder = (_SourceFolder.."\\temp");

	Folder.Create(TempFolder);
	
	for Count = 1, Grid.GetRowCount("Grid1") do
		local Path = String.Replace(Grid.GetCellText("Grid1", Count, 2), WorkingFolder, "", true);
		local Split = String.SplitPath(_SourceFolder.."\\temp\\"..Path);

		if(Split.Extension == "")then--FOLDER
			local BuildingPath = (_SourceFolder.."\\temp");
			for _, FolderName in pairs(DelimitedToTable(Path, "\\")) do
				Folder.Create(BuildingPath.."\\"..FolderName);
				BuildingPath = (BuildingPath.."\\"..FolderName);
			end
		else---------------------------FILE
			if(Folder.DoesExist(Split.Drive..Split.Folder))then
				File.Copy(Grid.GetCellText("Grid1", Count, 2), _SourceFolder.."\\temp\\"..Path, true, true, false, true, nil);
			else
				local BuildingPath = (_SourceFolder.."\\temp");
				local Split2 = String.SplitPath(Path);
				for _, FolderName in pairs(DelimitedToTable(String.Replace(Path, "\\"..Split2.Filename..Split2.Extension, "", true), "\\")) do
					Folder.Create(BuildingPath.."\\"..FolderName);
					BuildingPath = (BuildingPath.."\\"..FolderName);
				end
				
				if(Folder.DoesExist(Split.Drive..Split.Folder))then
					File.Copy(Grid.GetCellText("Grid1", Count, 2), _SourceFolder.."\\temp\\"..Path, true, true, false, true, nil);
				end
			end
		end
		
	end
else
	--error pls make stage 1 first.
end</Script>
