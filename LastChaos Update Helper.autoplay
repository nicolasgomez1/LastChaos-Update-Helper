<Args>number e_ID, table e_ItemInfo</Args>
<Script>if(e_ID ==  1)then
	if(tblMenu[1].Text == "Settings")then
		tblMenu[1].Text = ("Main");
		ShowHide({"Grid1"}, false);
		Group.SetVisible("ConfigPanel", true);
		ShowHide(tblConfig, true);
		Input.SetText("CONFIG_CLIENTFOLDER", String.AbbreviateFilePath(GetConfig("General", "ClientFolderPath"), 24));
		LoadProfiles();
	elseif(tblMenu[1].Text == "Main")then
		tblMenu[1].Text = ("Settings");
		ShowHide(tblConfig, false);
		ShowHide({"Grid1"}, true);
		Group.SetVisible("ConfigPanel", false);
	end
	
	Application.SetMenu(tblMenu);
end

if(e_ID == 2)then
	if(tblMenu[3].Text == "Start")then
		ShowHide(tblConfig, false);
		ShowHide({"Grid1"}, true);
		Group.SetVisible("ConfigPanel", false);
		
		tblMenu[1].Enabled = (false);
		tblMenu[3].Text = ("Stop");
		
		Window.SetText(Application.GetWndHandle(), "Working Folder : "..WorkingFolder);
		
		Watcher.Initialize("watcher1");
		Watcher.SetPath("watcher1", WorkingFolder);
		Watcher.SetFilter("watcher1", "*");
		Watcher.SetNotifyFilter("watcher1", "FileName, DirectoryName, Size, LastWrite");      
		Watcher.SetEventType("watcher1", "Changed");
		Watcher.SetLogErrors("watcher1");
		Watcher.SetIncludeSubdirectories("watcher1", true);
		Watcher.SetEnableRaisingEvents("watcher1", true);
		
		Watcher.Initialize("watcher2");
		Watcher.SetPath("watcher2", WorkingFolder);
		Watcher.SetFilter("watcher2", "*");
		Watcher.SetNotifyFilter("watcher2", "FileName, DirectoryName");      
		Watcher.SetEventType("watcher2", "Created, Deleted, Renamed");
		Watcher.SetLogErrors("watcher2");
		Watcher.SetIncludeSubdirectories("watcher2", true);
		Watcher.SetEnableRaisingEvents("watcher2", true);
		
		Page.StartTimer(1000, 10);
		
		if(Grid.GetRowCount("Grid1") &gt; 1)then
			local Ret = Dialog.Message("Warning", "The grid is not empty, do you want to empty it?", MB_YESNO, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
			if(Ret == 6)then
				Grid.DeleteNonFixedRows("Grid1", true);
			end
		end
		
		PrintLog("[Stage 1]\tFile watcher has been started At : '"..WorkingFolder.."'.", "", "INFO");
	elseif(tblMenu[3].Text == "Stop")then
		tblMenu[1].Enabled = (true);
		tblMenu[3].Text = ("Start");
		
		Window.SetText(Application.GetWndHandle(), "LastChaos Updater Helper");
		
		Watcher.UnInitialize("watcher1");
		Watcher.UnInitialize("watcher2");
		
		Page.StopTimer(1000);
		
		PrintLog("[Stage 1]\tFile watcher stopped.", "", "INFO");
		
		if(Grid.GetRowCount("Grid1") &gt; 1)then
			local Ret = Dialog.Message("Stage 1 Completed", "Do you want to continue to Stage 2?", MB_YESNO, MB_ICONINFORMATION, MB_DEFBUTTON1);
			if(Ret == 6)then
				Stage2();
			elseif(Ret == 7)then
				tblMenu[5].Enabled = (true);
				Application.SetMenu(tblMenu);
			end
		end
	end

	Application.SetMenu(tblMenu);
end

if(e_ID == 3)then
	Stage2();
end

if(e_ID == 4)then
	Stage3();
end</Script>
