		StatusDlg.Show(MB_ICONNONE, false);
		StatusDlg.SetTitle("Creating Grid Log file");
		StatusDlg.SetMessage("Please Wait...");
		StatusDlg.ShowCancelButton(false, "Cancel");
		StatusDlg.ShowProgressMeter(false);
	
		local GridLogs = ("----------------------------------------("..System.GetDate(DATE_FMT_ISO).." - "..System.GetTime(TIME_FMT_MIL)..")----------------------------------------");
		
