<Event>
<Name>On Click</Name>
<Args/>
<Script>local HOST = Input.GetText("CONFIG_FTP_HOST");
local USERNAME = Input.GetText("CONFIG_FTP_USERNAME");

if(HOST ~= "")then
	StatusDlg.Show(MB_ICONNONE, false);
	StatusDlg.ShowProgressMeter(false);
	StatusDlg.SetTitle("FTP Test Connection");
	StatusDlg.SetMessage("Please wait...");
	
	local Ret = Dialog.Message("FTP Test Connection", "You want to do a connection test in anonymous mode?", MB_YESNO, MB_ICONQUESTION, MB_DEFBUTTON1);

	if(Ret == 7)then
		if(FTP.IsValidHostname(HOST))then
			FTP.Connect(HOST, USERNAME, Input.GetText("CONFIG_FTP_PASSWORD"), "", true);
			local FTPError = Application.GetLastError();
			if(FTPError ~= FTP.OK)then
	    		PrintLog("FTP Connection Test -&gt; ".._tblErrorMessages[FTPError], "", "INFO");FTP.GetLog()
	    		--PrintLog("FTP Connection Test -&gt; "..FTP.GetLog(), "", "INFO");
	    	else
	    		PrintLog("FTP Connection Test -&gt; Connection with the ftp server was established successfully.", "", "SUCCESS");
	    		FTP.Disconnect();
			end
		else
			Dialog.TimedMessage("FTP Connection Test", "Hostname not valid.", 1200, MB_ICONINFORMATION);
			Input.SetProperties("CONFIG_FTP_HOST", {BackgroundColor = Math.HexColorToNumber(Theme.RedColor)});
			Page.SetFocus("CONFIG_FTP_HOST");
		end
	elseif(Ret == 6)then
		FTP.Connect(HOST);
		local FTPError = Application.GetLastError();
		if(FTPError ~= FTP.OK)then
	    	PrintLog("FTP Connection Test -&gt; ".._tblErrorMessages[FTPError], "", "INFO");
	    	--PrintLog("FTP Connection Test -&gt; "..FTP.GetLog(), "", "INFO");
	    else
	    	PrintLog("FTP Connection Test -&gt; Connection with the ftp server was established successfully.", "", "SUCCESS");
	    	FTP.Disconnect();
		end
	end
	StatusDlg.SetStatusText("Connection Test finished.");
	StatusDlg.Hide();
else
	Input.SetProperties("CONFIG_FTP_HOST", {BackgroundColor = Math.HexColorToNumber(Theme.RedColor)});
	Page.SetFocus("CONFIG_FTP_HOST");
end</Script>
