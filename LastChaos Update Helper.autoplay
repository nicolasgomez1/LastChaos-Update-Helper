function LoadProfiles()
	ComboBox.ResetContent("CONFIG_FTP_PROFILESELECTOR");
	
	for _, Profile in pairs(INIFile.GetValueNames(SettingsFile, "FTP_Profiles")) do
		local Value = DelimitedToTable(GetConfig("FTP_Profiles", Profile), ",");

		ComboBox.AddItem("CONFIG_FTP_PROFILESELECTOR", Profile, Value[1]..","..Value[2]..","..Value[3]..","..Value[4]..","..Value[5]);
		
		local Selected = GetConfig("General", "SelectedProfile");
		if(Selected ~= "")then
			if(Profile == Selected)then
				ComboBox.SetSelected("CONFIG_FTP_PROFILESELECTOR", _);
			end
		end
	end
