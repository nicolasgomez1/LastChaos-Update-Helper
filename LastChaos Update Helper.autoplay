<Args>number e_ID, table e_ItemInfo</Args>
<Script>if(e_ID == 1)then-- Settings
	DialogEx.Show("settings", true, nil, nil);
end

if(e_ID == 2)then-- Stage 1 (Start/Stop)
	if(WorkingFolder ~= "")then
		if(Folder.DoesExist(WorkingFolder))then
			local tblMenu = Application.GetMenu();
			
			local Status = (e_ItemInfo.Text);
			if(Status == "Stage 1 (Start)")then
				if(GridEx.GetNumberRows("Grid") &gt; 0)then
					local Ret = Dialog.Message("Warning", "The Grid is not empty, do you want to empty it?", MB_YESNO, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
					if(Ret == 6)then
						for RowID = 0, GridEx.GetNumberRows("Grid") do
							GridEx.DeleteRow("Grid", RowID);
						end
					end
				end
				
				Watcher.Initialize("watcher1");
				Watcher.SetPath("watcher1", WorkingFolder);
				Watcher.SetFilter("watcher1", "*");
				Watcher.SetNotifyFilter("watcher1", "FileName, DirectoryName, Size, LastWrite");      
				Watcher.SetEventType("watcher1", "Changed");
				Watcher.SetLogErrors("watcher1");
				Watcher.SetIncludeSubdirectories("watcher1", true);
				Watcher.SetEnableRaisingEvents("watcher1", true);
				
				Watcher.Initialize("watcher2");
				Watcher.SetPath("watcher2", WorkingFolder);
				Watcher.SetFilter("watcher2", "*");
				Watcher.SetNotifyFilter("watcher2", "FileName, DirectoryName");      
				Watcher.SetEventType("watcher2", "Created, Deleted, Renamed");
				Watcher.SetLogErrors("watcher2");
				Watcher.SetIncludeSubdirectories("watcher2", true);
				Watcher.SetEnableRaisingEvents("watcher2", true);
				
				Page.StartTimer(1000, 1);
				
				Step = ("1.1");
				StatusCount = ("*");
				Page.StartTimer(500, 2);
				
				Window.SetText(Application.GetWndHandle(), "LastChaos Update Helper\tWorking Folder: "..WorkingFolder);
				
				PrintLog("Stage 1 -&gt; File Watcher Started.", "", "INFO");
				
				tblMenu[1].SubMenu[1].Text = ("Stage 1 (Stop)");
				tblMenu[1].SubMenu[3].Enabled = (false);
				tblMenu[1].SubMenu[5].Enabled = (false);
				tblMenu[2].Enabled = (false);
			elseif(Status == "Stage 1 (Stop)")then
				tblMenu[2].Enabled = (true);
				
				Window.SetText(Application.GetWndHandle(), "LastChaos Update Helper");
				
				Watcher.UnInitialize("watcher1");
				Watcher.UnInitialize("watcher2");
				Page.StopTimer(1);
				
				Tree.SetNode("Tree1", "1.1", {Text = "Detecting changes in Directory"});
				Page.StopTimer(2);
				
				PrintLog("Stage 1 -&gt; File Watcher Stopped.", "", "INFO");
				
				tblMenu[1].SubMenu[1].Text = ("Stage 1 (Start)");
				
				if(GridEx.GetNumberRows("Grid") &gt; 0)then
					PrintLog("Stage 1 -&gt; Stage 2 is now enabled.", "", "SUCCESS");
					
					Dialog.TimedMessage("Stage 1 -&gt; SUCCESS", "Stage 2 is now Enabled!", 1200, MB_ICONINFORMATION);
					
					tblMenu[1].SubMenu[3].Enabled = (true);
				else
					tblMenu[1].SubMenu[3].Enabled = (false);
					tblMenu[1].SubMenu[5].Enabled = (false);
				end
			end
		
			Application.SetMenu(tblMenu);
		end
	else
		PrintLog("First of all you must define the client folder.", "", "ERROR");
	end
end

if(e_ID == 3)then-- Stage 2
	local tblMenu = Application.GetMenu();
	
	tblMenu[2].Enabled = (true);
	
	if(GridEx.GetNumberRows("Grid") &gt; 0)then
		StatusDlg.Show(MB_ICONNONE, false);
		StatusDlg.SetAutoSize(true);
		StatusDlg.SetTitle("Stage 2 Previous preparations");
		StatusDlg.SetMessage("Cleaning temporary working folder...");
		StatusDlg.ShowProgressMeter(true);
		StatusDlg.SetMeterRange(0, 100);

		Folder.DeleteTree(TempFolder.."\\", FolderDeleteTreeCallback);
		
		Folder.Create(TempFolder);
		
		PrintLog("Stage 2 -&gt; Temporary Folder emptied and ready to work!", "", "SUCCESS");
		
		Step = ("2.1");
		StatusCount = ("*");
		Page.StartTimer(500, 2);
		
		StatusDlg.SetTitle("Stage 2 Replicating Folders/Files structure");
		StatusDlg.SetMessage("Copying Folders/Files, Please wait...");
		
		for RowID = 0, GridEx.GetNumberRows("Grid") do
			local RawPath	= GridEx.GetCellText("Grid", 2, RowID);
			local Path		= String.Replace(RawPath, WorkingFolder, "", true);
			local Split		= String.SplitPath(TempFolder.."\\"..Path);
			
			if(Split.Extension == "")then--FOLDER
				local BuildingPath = (TempFolder);
				
				for _, FolderName in pairs(DelimitedToTable(Path, "\\")) do
					Folder.Create(BuildingPath.."\\"..FolderName);
					BuildingPath = (BuildingPath.."\\"..FolderName);
				end
			else---------------------------FILE
				if(Folder.DoesExist(Split.Drive..Split.Folder))then
					File.Copy(RawPath, TempFolder..Path, true, true, false, true, FileCopyCallback);
				else
					local BuildingPath = (TempFolder);
					
					for _, FolderName in pairs(DelimitedToTable(String.Replace(Path, "\\"..Split.Filename..Split.Extension, "", true), "\\")) do
						Folder.Create(BuildingPath.."\\"..FolderName);
						BuildingPath = (BuildingPath.."\\"..FolderName);
					end
					
					if(Folder.DoesExist(Split.Drive..Split.Folder))then
						File.Copy(RawPath, TempFolder..Path, true, true, false, true, FileCopyCallback);
					end
				end
			end
		end
		
		StatusDlg.Hide();
		
		Tree.SetNode("Tree1", "2.1", {Text = "Replicating Folders/Files structure"});
		Page.StopTimer(2);
		
		PrintLog("Stage 2 -&gt; Folders/Files Copied to Temporary working folder. Stage 3 is now enabled.", "", "SUCCESS");
		
		Dialog.TimedMessage("Stage 2 -&gt; SUCCESS", "Stage 3 is now Enabled!", 1200, MB_ICONINFORMATION);

		tblMenu[1].SubMenu[5].Enabled = (true);
	end
	
	Application.SetMenu(tblMenu);
end

if(e_ID == 4)then-- Stage 3
	local tblMenu = Application.GetMenu();
	
	tblMenu[2].Enabled = (false);
	
	local Continue = (false);
	local ConnectionData;
	local Version;
	local NewVersion;
	
	if(FTP.IsConnected())then
		FTP.Disconnect();
	end
	
	local tblProfiles = ({});
	local SelectedProfile = (0);
		
	for _, Profile in pairs(INIFile.GetValueNames(SettingsFile, "Profiles")) do
		tblProfiles[_] = (Profile);
		if(Profile == GetConfig("General", "SelectedProfile"))then
			SelectedProfile = (_);
		end
	end
	
	local Profile = Dialog.ComboBox("Stage 3", "Select the Profile you want to use", tblProfiles, tblProfiles[SelectedProfile], false, false, MB_ICONNONE);
	if(Profile == "CANCEL")then
		Continue = (false);
	else
		PrintLog("Stage 3 -&gt; Selected Profile : '"..Profile.."'.", "", "INFO");
		ConnectionData = DelimitedToTable(GetConfig("Profiles", Profile), ",");
	
		Continue = (true);
	end
	
	if(Continue == true)then--FTP
		Step = ("3.1");
		StatusCount = ("*");
		Page.StartTimer(500, 2);
		
		StatusDlg.Show(MB_ICONNONE, false);
		StatusDlg.ShowProgressMeter(false);
		StatusDlg.SetAutoSize(true);
		StatusDlg.SetTitle("Stage 3 FTP Connection");
		StatusDlg.SetMessage("Trying to connect to the FTP server...");
		StatusDlg.SetStatusText("Hostname\t:\t"..ConnectionData[1].."\r\nUsername\t:\t"..ConnectionData[2].."\r\nPassword\t:\t"..ConnectionData[3]);
		
		FTP.Connect(ConnectionData[1], ConnectionData[2], Crypto.BlowfishDecryptString(ConnectionData[3], "iddqd"), "", true);
		local FTPError = Application.GetLastError();
		if(FTPError ~= FTP.OK)then
			Tree.SetNode("Tree1", "3.1", {Text = "Connect to FTP"});
			Page.StopTimer(2);
			
			StatusDlg.SetMessage("Connection to FTP Server failed...");
			StatusDlg.SetStatusText(FTPError);
			PrintLog("Stage 3 -&gt; Failed to connect to FTP Server(".._tblErrorMessages[FTPError].."). Cannot continue.", "", "ERROR");
			
			Continue = (false);
		elseif(FTPError == FTP.OK)then
			Tree.SetNode("Tree1", "3.1", {Text = "Connect to FTP"});
			Page.StopTimer(2);
			
			StatusDlg.SetMessage("Connection to FTP server established!");
			StatusDlg.SetStatusText("");
			PrintLog("Stage 3 -&gt; Connected to FTP Server!", "", "SUCCESS");
			---
			StatusDlg.SetMessage("Looking for 'patches' Directory...");
			StatusDlg.SetStatusText("'"..ConnectionData[4].."'");
			
			Step = ("3.1.1");
			StatusCount = ("*");
			Page.StartTimer(500, 2);
			
			FTP.ChangeDir(ConnectionData[4]);
			local FTPError = FTP.GetLastResponse();
			if(String.Left(FTPError, 3) == "550")then
				FTP.Disconnect();
				
				StatusDlg.SetMessage("Looking for 'patches' Directory failed...");
				StatusDlg.SetStatusText(String.Replace(FTPError, "\r\n", "", false));
				
				Tree.SetNode("Tree1", "3.1.1", {Text = "Looking for Patches Directory"});
				Page.StopTimer(2);
				
				PrintLog("Stage 3 -&gt; FTP Error : "..String.Replace(FTPError, "\r\n", "", false).." Cannot continue.", "", "ERROR");
				Continue = (false);
				--[[while(true)do --esto en principio funciona pero puede dar problemas, lo mejor es que al hacer el test de conexion informe si no existe la carpeta y pregunta si desea crearla y blablabla
					local Ret = Dialog.Message("Stage 3 -&gt; Directory was not found or does not exist.", "Do you want to create it?", MB_YESNO, MB_ICONINFORMATION, MB_DEFBUTTON1);
					if(Ret == "CANCEL" or Ret == 7)then
						PrintLog("Stage 3 -&gt; FTP Error : "..String.Replace(FTPError, "\r\n", "", false).." Cannot continue.", "", "ERROR");

						Continue = (false);
						
						break;
					elseif(Ret == 6)then
						local Directory = DelimitedToTable(ConnectionData[4], "//");
						for _, FolderName in pairs(Directory) do
							FTP.MakeDir(FolderName);
							local FTPError = Application.GetLastError();
							if(FTPError ~= FTP.OK)then
    							Dialog.Message("FTP Error", _tblErrorMessages[FTPError], MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
    							PrintLog("Stage 3 -&gt; FTP Error : ".._tblErrorMessages[FTPError].." Cannot continue.", "", "ERROR");
    							
    							Continue = (false);
    							break;
    						else
    							--folder created
    							FTP.ChangeDir("/"..FolderName);
							end
						end
						
						break;
					end
				end]]
			else
				Tree.SetNode("Tree1", "3.1.1", {Text = "Looking for Patches Directory"});
				Page.StopTimer(2);
				
				StatusDlg.SetStatusText("Directory: '"..ConnectionData[4].."' was found!");
				PrintLog("Stage 3 -&gt; Directory : '"..ConnectionData[4].."' was found!", "", "SUCCESS");
				
				Step = ("3.1.1.1");
				StatusCount = ("*");
				Page.StartTimer(500, 2);
				
				StatusDlg.SetMessage("Looking for Version file(version.txt)...");
				StatusDlg.SetStatusText("'"..ConnectionData[4].."/version.txt'");
				
				local FileList = FTP.ListFiles();
				if(#FileList == 0)then
					StatusDlg.SetMessage("Looking for Version file(version.txt) failed...");
					StatusDlg.SetStatusText("Directory: '"..ConnectionData[4].."' is empty.");
					
					local Ret = Dialog.Message("Stage 3", "Version file(version.txt) does not exist, do you want to create it?", MB_YESNO, MB_ICONINFORMATION, MB_DEFBUTTON1);
					if(Ret == "CANCEL" or Ret == 7)then
						FTP.Disconnect();
						
						PrintLog("Stage 3 -&gt; Directory : '"..ConnectionData[4].."' is empty. Cannot continue.", "", "ERROR");
						
						Continue = (false);
					elseif(Ret == 6)then
						TextFile.WriteFromString(_SourceFolder.."\\NewVersionFile.txt.TEMP", "0", false);
						
						StatusDlg.SetMessage("Uploading New Version file(version.txt)...");
						StatusDlg.ShowProgressMeter(true);
    					StatusDlg.SetMeterRange(0, 100);
						
						FTP.Upload(_SourceFolder.."\\NewVersionFile.txt.TEMP", "version.txt", FTPUploadCallback);
						local FTPError = Application.GetLastError();
						if(FTPError ~= FTP.OK)then
							PrintLog("Stage 3 -&gt; FTP Error : ".._tblErrorMessages[FTPError].." Cannot continue.", "", "ERROR");
							
							Continue = (false);					
						else
							PrintLog("Stage 3 -&gt; Version file(version.txt) was created. The assigned version value is 0.", "", "SUCCESS");
						end
						
						StatusDlg.ShowProgressMeter(false);
						
						File.Delete(_SourceFolder.."\\NewVersionFile.txt.TEMP", false, false, false, nil);
					end
					
					Tree.SetNode("Tree1", "3.1.1.1", {Text = "Looking for Version file"});
					Page.StopTimer(2);
				else
					local VersionFileFound = (false);
					
					for Count = 1, #FileList do
						if(FileList[Count].Name == "version.txt")then
							VersionFileFound = (true);
							break;
						end
					end
					
					if(VersionFileFound == false)then
						StatusDlg.SetMessage("Looking for Version file(version.txt) failed...");
						StatusDlg.SetStatusText("Version file(version.txt) does not found in 'patches' Directory.");
						
						PrintLog("Stage 3 -&gt; Version file(version.txt) does not exist on 'patches' Directory. Cannot continue.", "", "ERROR");
						
						Continue = (false);
					else
						StatusDlg.SetMessage("Looking for Version file(version.txt)...");
						StatusDlg.SetStatusText("Version file(version.txt) was found.");
						
						PrintLog("Stage 3 -&gt; Version file(version.txt) was found!", "", "SUCCESS");
					end
					
					Tree.SetNode("Tree1", "3.1.1.1", {Text = "Looking for Version file"});
					Page.StopTimer(2);
				end
			end
		end
	end--END FTP
	
	if(Continue == true)then--MySQL
		Step = ("3.2");
		StatusCount = ("*");
		Page.StartTimer(500, 2);
		
		StatusDlg.SetTitle("Stage 3 MySQL Connection");
		StatusDlg.SetMessage("Trying to connect to Database...");
		StatusDlg.SetStatusText("Hostname\t:\t"..ConnectionData[5].."\r\nUsername\t:\t"..ConnectionData[6].."\r\nPassword\t:\t"..ConnectionData[7].."\r\nDB Data\t:\t"..ConnectionData[8]);
		
		MySQLConnection, MySQLError = MySQL:connect(ConnectionData[8], ConnectionData[6], ConnectionData[7], ConnectionData[5], 3306);
		if(MySQLError ~= nil)then
			StatusDlg.SetMessage("Connection to MySQL failed...");
			StatusDlg.SetStatusText("MySQL Error : "..String.Right(MySQLError, String.Length(MySQLError)-8));
			PrintLog("Stage 3 -&gt; MySQL Error : "..String.Right(MySQLError, String.Length(MySQLError)-8).." Cannot continue.", "", "ERROR");
			
			Continue = (false);
		else
			StatusDlg.SetMessage("Connection to MySQL server established!");
			StatusDlg.SetStatusText("Database: '"..ConnectionData[8].."'.");
			PrintLog("Stage 3 -&gt; Connected to MySQL Server!", "", "SUCCESS");
			---
			StatusDlg.SetMessage("Looking for 't_clientversion' table...");
			StatusDlg.SetStatusText("");
			
			MySQLCursor, MySQLError = MySQLConnection:execute("SELECT a_min a_max FROM "..ConnectionData[8]..".t_clientversion");
			if(MySQLError ~= nil)then
				StatusDlg.SetStatusText("Table: 't_clientversion' not found.");
				
				local Ret = Dialog.Message("Stage 3", "Table 't_clientversion' does not exist, do you want to create it?", MB_YESNO, MB_ICONINFORMATION, MB_DEFBUTTON1);
				if(Ret == "CANCEL" or Ret == 7)then
					MySQLConnection:close();

					PrintLog("Stage 3 -&gt; MySQL Error : "..String.Right(MySQLError, String.Length(MySQLError)-8).." Cannot continue.", "", "ERROR");
					
					Continue = (false);
				else
					MySQLCursor, MySQLError = MySQLConnection:execute("CREATE TABLE t_clientversion (a_min int(11) NOT NULL DEFAULT 0, a_max int(11) NOT NULL DEFAULT 0, PRIMARY KEY (a_max, a_min))");
					if(MySQLError ~= nil)then
						PrintLog("Stage 3 -&gt; MySQL Error : "..String.Right(MySQLError, String.Length(MySQLError)-8).." Cannot continue.", "", "ERROR");
						
						Continue = (false);
					else
						PrintLog("Stage 3 -&gt; Table 't_clientversion' has been created!", "", "SUCCESS");
						MySQLCursor, MySQLError = MySQLConnection:execute("INSERT INTO "..ConnectionData[8]..".t_clientversion (a_min, a_max) VALUES (0, 0)");
						if(MySQLError ~= nil)then
							PrintLog("Stage 3 -&gt; MySQL Error : "..String.Right(MySQLError, String.Length(MySQLError)-8).." Cannot continue.", "", "ERROR");
							
							Continue = (false);
						else
							PrintLog("Stage 3 -&gt; Default values were defined for columns 'a_min' and 'a_max'.", "", "SUCCESS");
							
						end
					end
				end
			else
				StatusDlg.SetMessage("Looking for 't_clientversion' table...");
				StatusDlg.SetStatusText("Table: 't_clientversion' was found.");
				PrintLog("Stage 3 -&gt; Table : 't_clientversion' was found!", "", "SUCCESS");
				
				PrintLog("Stage 3 -&gt; All prerequisites found.", "", "SUCCESS");
			end
		end
		
		Tree.SetNode("Tree1", "3.2", {Text = "Connect to Database"});
		Page.StopTimer(2);
	end--End MySQL
	
	if(Continue == true)then
		Step = ("3.3");
		StatusCount = ("*");
		Page.StartTimer(500, 2);
		
		StatusDlg.SetTitle("Stage 3 Getting Version");
		StatusDlg.SetMessage("Downloading Version file(version.txt)...");
		StatusDlg.SetStatusText("");
		StatusDlg.ShowProgressMeter(true);
		StatusDlg.SetMeterRange(0, 100);
		
		FTP.Download("version.txt", _SourceFolder.."\\version.txt.BACKUP", FTPDownloadCallback);
		local FTPError = Application.GetLastError();
		if(FTPError ~= FTP.OK)then
			PrintLog("Stage 3 -&gt; FTP Error : ".._tblErrorMessages[FTPError].." Cannot continue.", "", "ERROR");
			
			Continue = (false);
		elseif(FTPError == FTP.OK)then
			PrintLog("Stage 3 -&gt; Version file downloaded successfully.", "", "SUCCESS");
			
			local Version = tonumber(TextFile.ReadToString(_SourceFolder.."\\version.txt.BACKUP"));
			if(Version == "")then
				PrintLog("Stage 3 -&gt; Version file is empty, 0 was assigned as default value.", "", "WARNING");
				
				NewVersion = (0);
			else
				NewVersion = (Version+1);
			end
		end
		
		Tree.SetNode("Tree1", "3.3", {Text = "Getting Version from File"});
		Page.StopTimer(2);
	end
	
	if(Continue == true)then
		Step = ("3.4");
		StatusCount = ("*");
		Page.StartTimer(500, 2);
		
		ZipPath = (_SourceFolder.."\\"..String.Replace(System.GetDate(DATE_FMT_ISO), "-", "_", true).."-"..String.Replace(System.GetTime(TIME_FMT_MIL), ":", "_", true).."-"..NewVersion..".zip");
		
		StatusDlg.SetTitle("Stage 3 Creating Update file");
		StatusDlg.SetMessage("Compressing: "..ZipPath);
		StatusDlg.SetStatusText("Please Wait...");
		--ver si el ZipEx es mas rapido###############################################################################################
		Zip.Add(ZipPath, {TempFolder.."\\*.*"}, true, "", 5, ZipCallback, true);
		local ZIPError = Application.GetLastError();
		if(ZIPError ~= 0)then
			PrintLog("Stage 3 -&gt; ZIP Error : ".._tblErrorMessages[ZIPError].." Cannot continue.", "", "ERROR");
			
			Continue = (false);
		else
			local Split = String.SplitPath(ZipPath);
			PrintLog("Stage 3 -&gt; Update file ('"..Split.Filename..Split.Extension.."') was created successfully.", "", "SUCCESS");
			---
			StatusDlg.SetMessage("Update Version file...");
			StatusDlg.SetStatusText("New Version: "..NewVersion);
			
			TextFile.WriteFromString(_SourceFolder.."\\NewVersionFile.txt.TEMP", NewVersion, false);
			local TXTError = Application.GetLastError();
			if(TXTError ~= 0)then
				PrintLog("Stage 3 -&gt; ".._tblErrorMessages[TXTError].." Cannot continue.", "", "ERROR");
				
				Continue = (false);
			else
				PrintLog("Stage 3 -&gt; New Version file was created!", "", "SUCCESS");
			end
		end
		
		Tree.SetNode("Tree1", "3.4", {Text = "Creating Update files"});
		Page.StopTimer(2);
	end
	
	if(Continue == true)then
		Step = ("3.5");
		StatusCount = ("*");
		Page.StartTimer(500, 2);
		
		StatusDlg.SetTitle("Stage 3 Uploading Files to server");
		StatusDlg.SetMessage("Uploading: "..NewVersion..".zip");
		StatusDlg.SetStatusText("Please Wait...");
		
		FTP.Upload(ZipPath, NewVersion..".zip", FTPUploadCallback);
		local FTPError = Application.GetLastError();
		if(FTPError ~= FTP.OK)then
			PrintLog("Stage 3 -&gt; FTP Error : ".._tblErrorMessages[FTPError].." Cannot continue.", "The Update file could not be Uploaded to Server.", "ERROR");
			
			Continue = (false);
		elseif(FTPError == FTP.OK)then
			PrintLog("Stage 3 -&gt; Update file ('"..NewVersion..".zip') was uploaded to server successfully!", "", "SUCCESS");
		end
		
		Tree.SetNode("Tree1", "3.5", {Text = "Uploading Update to Server"});
		Page.StopTimer(2);
	end
	
	if(Continue == true)then
		Step = ("3.6");
		StatusCount = ("*");
		Page.StartTimer(500, 2);
		
		StatusDlg.SetMessage("Uploading: version.txt");
		StatusDlg.SetStatusText("Please Wait...");
		
		FTP.Upload(_SourceFolder.."\\NewVersionFile.txt.TEMP", "version.txt", FTPUploadCallback);
		local FTPError = Application.GetLastError();
		if(FTPError ~= FTP.OK)then
			PrintLog("Stage 3 -&gt; FTP Error : ".._tblErrorMessages[FTPError].." Cannot continue.", "The Version file could not be Uploaded to Server('"..NewVersion..".zip' will be removed from the server.).", "ERROR");

			FTP.Delete(NewVersion..".zip");
			
			Continue = (false);
		elseif(FTPError == FTP.OK)then
			PrintLog("Stage 3 -&gt; Version file ('version.txt') was uploaded to server successfully!", "", "SUCCESS");
		end

		Tree.SetNode("Tree1", "3.6", {Text = "Uploading New Version file"});
		Page.StopTimer(2);
	end
	
	StatusDlg.ShowProgressMeter(false);
	
	if(Continue == true)then
		Step = ("3.7");
		StatusCount = ("*");
		Page.StartTimer(500, 2);
		
		StatusDlg.SetMessage("Updating Database values...");
		StatusDlg.SetStatusText("Table: 't_clientversion'...\r\nUpdating 'a_min' to: "..NewVersion.."\r\nUpdating 'a_max' to: "..(NewVersion+1));
		
		MySQLCursor, MySQLError = MySQLConnection:execute("UPDATE "..ConnectionData[8]..".t_clientversion SET a_min='"..NewVersion.."', a_max='"..(NewVersion+1).."'");
		if(MySQLError ~= nil)then
			PrintLog("Stage 3 -&gt; MySQL Error : "..String.Right(MySQLError, String.Length(MySQLError)-8).." Cannot continue.", "Update file('"..NewVersion..".zip') will be removed and Version file will be reverted.", "ERROR");
			
			StatusDlg.SetTitle("Reverting Update");
			StatusDlg.SetMessage("Removing '"..NewVersion..".zip' from Server...");
			
			FTP.Delete(NewVersion..".zip");
			local FTPError = Application.GetLastError();
			if(FTPError ~= FTP.OK)then
				PrintLog("Stage 3 -&gt; FTP Error : ".._tblErrorMessages[FTPError], "", "ERROR");
			elseif(FTPError == FTP.OK)then
				PrintLog("Stage 3 -&gt; "..NewVersion..".zip was removed successfully!", "", "SUCCESS");
			end
			
			StatusDlg.SetMessage("Reverting Version file from Server...");
			
			FTP.Upload(_SourceFolder.."\\version.txt.BACKUP", "version.txt", FTPUploadCallback);
			local FTPError = Application.GetLastError();
			if(FTPError ~= FTP.OK)then
				PrintLog("Stage 3 -&gt; FTP Error : ".._tblErrorMessages[FTPError].." FUCK IT BYE BYE.", "Revert the Version file(version.txt) file on the server manually.", "ERROR");
			elseif(FTPError == FTP.OK)then
				PrintLog("Stage 3 -&gt; Version file reverted successfully!", "", "SUCCESS");
			end
			
			Continue = (false);
		else
			PrintLog("Stage 3 -&gt; Table : 't_clientversion' was updated successfully!", "New min version: "..NewVersion.."\r\nNew max version: "..NewVersion+1, "SUCCESS");
			
			MySQLConnection:close();
			FTP.Disconnect();
			
			PrintLog("Stage 3 -&gt; Process completed successfully, the Update is ready!", "", "SUCCESS");
		end
		
		Tree.SetNode("Tree1", "3.7", {Text = "Updating Database values"});
		Page.StopTimer(2);
	end
	
	if(Continue == true)then
		tblMenu[1].SubMenu[3].Enabled = (false);
		tblMenu[1].SubMenu[5].Enabled = (false);
		
		if(GetConfig("General", "CleanAfterUpload") == "true")then
			local Split = String.SplitPath(ZipPath);
			if(File.DoesExist(ZipPath))then
				StatusDlg.SetTitle("Stage 3 Cleaning residue");
				StatusDlg.SetMessage("Deleting residual update file("..Split.Filename..Split.Extension..")...");
				StatusDlg.SetStatusText("Please Wait...");
				StatusDlg.ShowProgressMeter(true);
				
				File.Delete(ZipPath, false, false, false, FileDeleteCallback);
			end
		end
	else
		if(File.DoesExist(ZipPath))then
			local Split = String.SplitPath(ZipPath);
			PrintLog("Stage 3 -&gt; The process could not be completed, but '"..Split.Filename..Split.Extension.."' will remain as a remnant, in case you want to upload it manually.", "", "INFO");
		end
	end
	
	StatusDlg.SetTitle("Stage 3 Cleaning residue");
	StatusDlg.SetMessage("Cleaning TEMP Folder...");
	StatusDlg.SetStatusText("Please Wait...");
	StatusDlg.ShowProgressMeter(true);
				
	Folder.DeleteTree(TempFolder.."\\", FolderDeleteTreeCallback);	
	
	if(File.DoesExist(_SourceFolder.."\\version.txt.BACKUP"))then
		StatusDlg.SetMessage("Deleting residual file: 'version.txt.BACKUP'...");
		
		File.Delete(_SourceFolder.."\\version.txt.BACKUP", false, false, false, nil);
	end
	
	if(File.DoesExist(_SourceFolder.."\\NewVersionFile.txt.TEMP"))then
		StatusDlg.SetMessage("Deleting residual file: 'NewVersionFile.txt.TEMP'...");
		
		File.Delete(_SourceFolder.."\\NewVersionFile.txt.TEMP", false, false, false, nil);
	end
	
	StatusDlg.Hide();
	
	tblMenu[2].Enabled = (true);
	
	Application.SetMenu(tblMenu);
end

if(e_ID == 5)then
	if(WorkingFolder ~= "" and Folder.DoesExist(WorkingFolder))then
		PrintLog("Executing : '"..WorkingFolder.."'.", "", "INFO");
		Shell.Execute(WorkingFolder, "explore", "", "", SW_SHOWNORMAL, false);
	else
		PrintLog("Client Folder not defined.", "", "ERROR");
	end
end

if(e_ID == 6)then
	local TotalRows = GridEx.GetNumberRows("Grid");
	if(TotalRows &gt; 0)then
		StatusDlg.Show(MB_ICONNONE, false);
		StatusDlg.SetTitle("Creating Grid Log file");
		StatusDlg.SetMessage("Please Wait...");
		StatusDlg.ShowCancelButton(false, "Cancel");
		StatusDlg.ShowProgressMeter(false);
	
		local GridLogs = ("----------------------------------------("..System.GetDate(DATE_FMT_ISO).." - "..System.GetTime(TIME_FMT_MIL)..")----------------------------------------");
		for RowID = 0, TotalRows do
			local Event = GridEx.GetCellText("Grid", 0, RowID);
			if(Event ~= "DELETED")then
				GridLogs = (GridLogs.."\r\n"..Event.."\t"..GridEx.GetCellText("Grid", 1, RowID).."\t"..GridEx.GetCellText("Grid", 2, RowID));
			end
		end

		TextFile.WriteFromString(_SourceFolder.."\\"..String.Replace(System.GetDate(DATE_FMT_ISO), "-", "", false).."-"..String.Replace(System.GetTime(TIME_FMT_MIL), ":", "", false).."_Grid.log", GridLogs, false);
		Dialog.TimedMessage("Grid Log file", "Log file has been created!", 1200, MB_ICONINFORMATION);
		
		StatusDlg.Hide();
	end
end

if(e_ID == 7)then
	Shell.Execute("https://github.com/nicolasgomez1/LastChaos-Update-Helper", "open", "", "", SW_SHOWNORMAL, false);
end</Script>
