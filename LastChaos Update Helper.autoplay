<Script>if(e_ID == 2)then
	--esto se puede simplificar obteniendo el texto al principio, poniendolo en una variable y despues simplemente llamarla.
	if(Step == "1.1")then
		Tree.SetNode("Tree1", "1.1", {Text = "Detecting changes in Directory"..StatusCount});
	end
	
	if(Step == "2.1")then
		Tree.SetNode("Tree1", "2.1", {Text = "Replicating Folders/Files structure"..StatusCount});
	end
	
	if(Step == "3.1")then
		Tree.SetNode("Tree1", "3.1", {Text = "Connect to FTP"..StatusCount});
	end
	
	if(Step == "3.1.1")then
		Tree.SetNode("Tree1", "3.1.1", {Text = "Looking for Patches Directory"..StatusCount});
	end
	
	if(Step == "3.1.1.1")then
		Tree.SetNode("Tree1", "3.1.1.1", {Text = "Looking for Version file"..StatusCount});
	end
	
	if(Step == "3.2")then
		Tree.SetNode("Tree1", "3.2", {Text = "Connect to Database"..StatusCount});
	end
	
	if(Step == "3.3")then
		Tree.SetNode("Tree1", "3.3", {Text = "Getting Version from File"..StatusCount});
	end
	
	if(Step == "3.4")then
		Tree.SetNode("Tree1", "3.4", {Text = "Creating Update files"..StatusCount});
	end
	
	if(Step == "3.5")then
		Tree.SetNode("Tree1", "3.5", {Text = "Uploading Update to Server"..StatusCount});
	end
	
	if(Step == "3.6")then
		Tree.SetNode("Tree1", "3.6", {Text = "Uploading New Version file"..StatusCount});
	end
	
	if(Step == "3.7")then
		Tree.SetNode("Tree1", "3.7", {Text = "Updating Database values"..StatusCount});
	end
	
	if(String.Length(StatusCount) == 1)then
		StatusCount = ("**");
	elseif(String.Length(StatusCount) == 2)then
		StatusCount = ("***");
	elseif(String.Length(StatusCount) == 3)then
		StatusCount = ("*");
	end
end

if(e_ID == 1)then
	loggedEvent1 = Watcher.GetLoggedEvent("watcher1");
	loggedEvent2 = Watcher.GetLoggedEvent("watcher2");
	
	local Status = ({});
	
	if(loggedEvent1.Type ~= nil)then
		table.insert(Status, 1, {Event = loggedEvent1.Type, Path = loggedEvent1.Path});
	end
	
	if(loggedEvent2.Type ~= nil)then
		if(loggedEvent2.Type == "Renamed")then
			table.insert(Status, 1, {Event = loggedEvent2.Type, OldPath = loggedEvent2.OldPath, Path = loggedEvent2.Path});
		else
			table.insert(Status, 1, {Event = loggedEvent2.Type, Path = loggedEvent2.Path});
		end
	end
	
	if(#Status &gt; 0)then
		local Split = String.SplitPath(Status[1].Path);
		local Name;
		
		if(Split.Extension == "")then
			Name = (Split.Filename);
		else
			Name = (Split.Filename..Split.Extension);
		end
		
		if(Status[1].Event == "Created")then-----------------------------------------------------------------CREATED &gt;&gt; READER
			local TotalRows = GridEx.GetNumberRows("Grid");
			local LastRowBGColor = GridEx.GetCellBackColor("Grid", 0, TotalRows-1);
			GridEx.SetNumberRows("Grid", TotalRows+1);
			local RowID = (TotalRows);
			local Color = (5459008);
	
			if(LastRowBGColor == 5459008)then--Light color
				Color = (4998456);
			elseif(LastRowBGColor == 4998456)then--Dark one
				Color = (5459008);
			end
			
			for _, Text in pairs({string.upper(Status[1].Event), Name, Status[1].Path}) do
				local Col = (_-1);
				GridEx.SetCellText("Grid", Col, RowID, Text);
				GridEx.SetCellTextColor("Grid", Col, RowID, 1604346);
				GridEx.SetCellBackColor("Grid", Col, RowID, Color);
				GridEx.SetCellBorderColor("Grid", Col, RowID, 6579300);
				GridEx.SetCellBorder("Grid", Col, RowID, GEX_BDR_BTHIN);
				if(Col == 2)then
					GridEx.SetCellAlignment("Grid", Col, RowID, GEX_ALIGNLEFT);
				else
					GridEx.SetCellAlignment("Grid", Col, RowID, GEX_ALIGNCENTER);
				end
				GridEx.SetCellAlignment("Grid", Col, RowID, GEX_ALIGNVCENTER);
				GridEx.RedrawCell("Grid", Col, RowID);
			end
		elseif(Status[1].Event == "Deleted")then-------------------------------------------------------------DELETED &gt;&gt; READY?
			for RowID = 0, GridEx.GetNumberRows("Grid") do
				if(String.Find(GridEx.GetCellText("Grid", 2, RowID), Status[1].Path, 1, false) ~= -1)then
					GridEx.SetCellText("Grid", 0, RowID, "DELETED");
					GridEx.RedrawCell("Grid", 0, RowID);
					GridEx.SetCellText("Grid", 1, RowID, "---");
					GridEx.RedrawCell("Grid", 1, RowID);
					GridEx.SetCellText("Grid", 2, RowID, "---");
					GridEx.RedrawCell("Grid", 3, RowID);
				end
			end
		elseif(Status[1].Event == "Renamed")then--------------------------------------------------------------RENAMED &gt;&gt; READY
			local AnyFound = (false);
			
			for RowID = 0, GridEx.GetNumberRows("Grid") do
				if(String.Find(GridEx.GetCellText("Grid", 2, RowID), Status[1].OldPath, 1, false) ~= -1)then
					GridEx.SetCellText("Grid", 0, RowID, "RENAMED");
					
					if(GridEx.GetCellText("Grid", 1, RowID) == String.SplitPath(Status[1].OldPath).Filename..String.SplitPath(Status[1].OldPath).Extension)then
						GridEx.SetCellText("Grid", 1, RowID, Name);
					end
	
					GridEx.SetCellText("Grid", 2, RowID, String.Replace(GridEx.GetCellText("Grid", 2, RowID), Status[1].OldPath, Status[1].Path, true));
					AnyFound = (true);
				end
			end
			
			if(AnyFound == false)then
				local TotalRows = GridEx.GetNumberRows("Grid");
				local LastRowBGColor = GridEx.GetCellBackColor("Grid", 0, TotalRows);
				GridEx.SetNumberRows("Grid", TotalRows+1);
				local RowID = (TotalRows);
				local Color = (5459008);
				
				if(LastRowBGColor == 5459008)then--Light color
					Color = (4998456);
				elseif(LastRowBGColor == 4998456)then--Dark one
					Color = (5459008);
				end
				
				for _, Text in pairs({string.upper(Status[1].Event), Name, Status[1].Path}) do
					local Col = (_-1);
					GridEx.SetCellText("Grid", Col, RowID, Text);
					GridEx.SetCellTextColor("Grid", Col, RowID, 1604346);
					GridEx.SetCellBackColor("Grid", Col, RowID, Color);
					GridEx.SetCellBorderColor("Grid", Col, RowID, 6579300);
					GridEx.SetCellBorder("Grid", Col, RowID, GEX_BDR_BTHIN);
					if(Col == 2)then
						GridEx.SetCellAlignment("Grid", Col, RowID, GEX_ALIGNLEFT);
					else
						GridEx.SetCellAlignment("Grid", Col, RowID, GEX_ALIGNCENTER);
					end
					GridEx.SetCellAlignment("Grid", Col, RowID, GEX_ALIGNVCENTER);
					GridEx.RedrawCell("Grid", Col, RowID);
				end
			end
		elseif(Status[1].Event == "Changed")then--------------------------------------------------------------CHANGED
			local AnyFound = (false);
			
			for RowID = 0, GridEx.GetNumberRows("Grid") do
				if(Status[1].Path == GridEx.GetCellText("Grid", 2, RowID))then
					GridEx.SetCellText("Grid", 0, RowID, "CHANGED");
					
					AnyFound = (true);
				end
			end
			
			if(AnyFound == false)then
				local TotalRows = GridEx.GetNumberRows("Grid");
				local LastRowBGColor = GridEx.GetCellBackColor("Grid", 0, TotalRows);
				GridEx.SetNumberRows("Grid", TotalRows+1);
				local RowID = (TotalRows);
				local Color = (5459008);
				
				if(LastRowBGColor == 5459008)then--Light color
					Color = (4998456);
				elseif(LastRowBGColor == 4998456)then--Dark one
					Color = (5459008);
				end
				
				for _, Text in pairs({string.upper(Status[1].Event), Name, Status[1].Path}) do
					local Col = (_-1);
					GridEx.SetCellText("Grid", Col, RowID, Text);
					GridEx.SetCellTextColor("Grid", Col, RowID, 1604346);
					GridEx.SetCellBackColor("Grid", Col, RowID, Color);
					GridEx.SetCellBorderColor("Grid", Col, RowID, 6579300);
					GridEx.SetCellBorder("Grid", Col, RowID, GEX_BDR_BTHIN);
					if(Col == 2)then
						GridEx.SetCellAlignment("Grid", Col, RowID, GEX_ALIGNLEFT);
					else
						GridEx.SetCellAlignment("Grid", Col, RowID, GEX_ALIGNCENTER);
					end
					GridEx.SetCellAlignment("Grid", Col, RowID, GEX_ALIGNVCENTER);
					GridEx.RedrawCell("Grid", Col, RowID);
				end
			end
		end
		
		--GridEx.SetColumnWidth("Grid", 0, 65);
		--GridEx.SetColumnWidth("Grid", 1, 200);
	
		GridEx.FitToWindow("Grid", 0, 2);
		
		GridEx.RedrawAll("Grid");
		
		Status = ({});
	end
end</Script>
