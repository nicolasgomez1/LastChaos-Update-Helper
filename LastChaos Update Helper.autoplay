<Event>
<Name>On Click</Name>
<Args/>
<Script>local HOSTNAME = Input.GetText("CONFIG_FTP_HOST");
local USERNAME = Input.GetText("CONFIG_FTP_USERNAME");
local PASSWORD = Input.GetText("CONFIG_FTP_PASSWORD");
local PATCHESFOLDER = Input.GetText("CONFIG_FTP_PATCHESFOLDER");

local MakeTest = (true);

function TestFTP()
	FTP.Connect(HOSTNAME, USERNAME, Input.GetText("CONFIG_FTP_PASSWORD"), "", true);
	local FTPError = Application.GetLastError();
	if(FTPError ~= FTP.OK)then
		PrintLog("FTP Connection Test -&gt; ".._tblErrorMessages[FTPError], "", "INFO");FTP.GetLog()
	else
		PrintLog("FTP Connection Test -&gt; Connected to FTP Server successfully.", "", "SUCCESS");
				
		local FolderExist = (true);
		local VersionFileExist = (false);
				
		FTP.ChangeDir(PATCHESFOLDER);
		local WaitForError = FTP.GetLastResponse();
		if(String.Left(WaitForError, 3) == "550")then
			FolderExist = (false);
		else
			local FileList = FTP.ListFiles();
			if(#FileList &gt; 0)then
				for Count = 1, #FileList do
					if(FileList[Count].Name == "version.txt")then
						VersionFileExist = (true);
						break;
					end
				end
			end
		end
				
		if(FolderExist == false)then
			PrintLog("FTP Connection Test -&gt; Folder : '"..PATCHESFOLDER.."' does not exist on Server or path is wrong.", "", "ERROR");
		else
			PrintLog("FTP Connection Test -&gt; Folder : '"..PATCHESFOLDER.."' was found.", "", "SUCCESS");
		end
				
		if(VersionFileExist == false)then
			PrintLog("FTP Connection Test -&gt; Version File(version.txt) does not exist or path is wrong.", "", "ERROR");
		else
			PrintLog("FTP Connection Test -&gt; Version File(version.txt) was found.", "", "SUCCESS");
		end
				
		FTP.Disconnect();
	end
end

if(HOSTNAME == "" or FTP.IsValidHostname(HOSTNAME) == false)then
	Input.SetProperties("CONFIG_FTP_HOST", {BackgroundColor = Math.HexColorToNumber(Theme.RedColor)});
	Page.SetFocus("CONFIG_FTP_HOST");
	MakeTest = (false);
end

if(USERNAME == "")then
	Input.SetProperties("CONFIG_FTP_USERNAME", {BackgroundColor = Math.HexColorToNumber(Theme.RedColor)});
	Page.SetFocus("CONFIG_FTP_USERNAME");
	MakeTest = (false);
end
