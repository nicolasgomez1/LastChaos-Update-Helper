<Script>require("FileWatcher");

SettingsFile = (_SourceFolder.."\\config.ini");

Step = (nil);
StatusCount = ("*");

TempFolder = (_SourceFolder.."\\TEMP");

ZipPath = ("");

function GetConfig(Section, Value)
	return INIFile.GetValue(SettingsFile, Section, Value);
end

WorkingFolder = GetConfig("General", "ClientFolderPath");

function FileDeleteCallback(Source, Deleted, Total)
	local Percent = (Deleted/Total*100);
    
   	StatusDlg.SetMeterPos(Percent);
    StatusDlg.SetStatusText("Deleted: "..Source.." "..Percent.."%");
	
	return true;
end

function ZipCallback(String, Percent, Status)
	if(Status == ZIP_STATUS_MAJOR)then
		StatusDlg.SetMessage("Compressing : "..ZipPath.." "..Percent.."%");
		StatusDlg.SetMeterPos(Percent);
	end
	
	if(Status == ZIP_STATUS_MINOR)then
		StatusDlg.SetStatusText("Compressing file: "..String.." "..Percent.."%");
		StatusDlg.SetMeterPos(Percent);
	end

	return true;
end

function FTPDownloadCallback(Bytes, Total)
    if(Total ~= 0)then
    	local Percent = (Bytes/Total*100);
    	
    	StatusDlg.SetMeterPos(Percent);
    	StatusDlg.SetStatusText("Downloaded: "..Percent.."%");
    end
end

function FTPUploadCallback(Bytes, Total)
    if(Total ~= 0)then
    	local Percent = (Bytes/Total*100);
    	
    	StatusDlg.SetMeterPos(Percent);
    	StatusDlg.SetStatusText("Uploaded: "..Percent.."%");
    end
end

function FileCopyCallback(Source, Destination, Copied, Total, FileCopied, FileTotal)
	StatusDlg.SetStatusText("Copying: "..Source.."\r\nTo: "..String.Replace(Destination, Folder.GetCurrent(), "", false).."\t"..(FileCopied/FileTotal*100).."%");
	StatusDlg.SetMeterPos(Copied/Total*100);
	
	return true;
end

function FolderDeleteTreeCallback(Source, Deleted, Total)
	StatusDlg.SetStatusText("Deleting: "..Source);
	StatusDlg.SetMeterPos(Deleted/Total*100);
	
	return true;
end

function LoadProfiles()
	ComboBox.ResetContent("CONFIG_PROFILESELECTOR");
	
	for _, Profile in pairs(INIFile.GetValueNames(SettingsFile, "Profiles")) do
		local Value = DelimitedToTable(GetConfig("Profiles", Profile), ",");

		ComboBox.AddItem("CONFIG_PROFILESELECTOR", Profile, Value[1]..","..Value[2]..","..Value[3]..","..Value[4]..","..Value[5]..","..Value[6]..","..Value[7]..","..Value[8]);
		
		local Selected = GetConfig("General", "SelectedProfile");
		if(Selected ~= "")then
			if(Profile == Selected)then
				ComboBox.SetSelected("CONFIG_PROFILESELECTOR", _);
			end
		end
	end
end

function DelimitedToTable(String, Delimiter)
	if not(Delimiter or #Delimiter &lt; 1)then
		return nil;
	end

	local tbl = ({});
	local sa = (String);
	local sD = ("");
	local nP = string.find(sa, Delimiter, 1, true);

	while(nP)do
		sD = string.sub(sa, 1, nP-1);
		table.insert(tbl, #tbl+1, sD);
		sa = string.sub(sa, nP+1, -1);
		nP = string.find(sa, Delimiter, 1, true);
	end

	if(sa ~= "")then
		Table.Insert(tbl, #tbl+1, sa)
	end

	return tbl;
end

function LoadProfiles()
	ComboBox.ResetContent("CONFIG_PROFILESELECTOR");
	
	for _, Profile in pairs(INIFile.GetValueNames(SettingsFile, "Profiles")) do
		local Value = DelimitedToTable(GetConfig("Profiles", Profile), ",");

		ComboBox.AddItem("CONFIG_PROFILESELECTOR", Profile, Value[1]..","..Value[2]..","..Value[3]..","..Value[4]..","..Value[5]..","..Value[6]..","..Value[7]..","..Value[8]);
		
		local Selected = GetConfig("General", "SelectedProfile");
		if(Selected ~= "")then
			if(Profile == Selected)then
				ComboBox.SetSelected("CONFIG_PROFILESELECTOR", _);
			end
		end
	end
end

function PrintLog(Text, Tooltip, Type)
	local Hint = ("");
	local ItemColor = ({Math.HexColorToNumber("0x38454C")});

	if(Type == "INFO")then			Type = ("&lt;font color='"..String.Replace("0x8AB4F8", "0x", "#", false).."'&gt;INFO &lt;/font&gt;");
	elseif(Type == "ERROR")then		Type = ("&lt;font color='"..String.Replace("0xF85149", "0x", "#", false).."'&gt;ERROR &lt;/font&gt;");
	elseif(Type == "SUCCESS")then	Type = ("&lt;font color='"..String.Replace("0x3FB950", "0x", "#", false).."'&gt;SUCCESS &lt;/font&gt;");
	elseif(Type == "WARNING")then	Type = ("&lt;font color='"..String.Replace("0xFDDF68", "0x", "#", false).."'&gt;WARNING &lt;/font&gt;");	end
	
	if(Tooltip ~= "")then	Hint = ("&lt;sup&gt;(*)&lt;/sup&gt;");	end
	
	if(ListBoxEx.GetTotalItems("Logs") == 0)then
		ItemColor = ({Math.HexColorToNumber("0x38454C")});
	else
		ItemColor = ListBoxEx.GetItemColor("Logs", ListBoxEx.GetTotalItems("Logs"), LBXISTATE_NORMAL);
	end
	
	if(ItemColor[1] == Math.HexColorToNumber("0x38454C"))then
		ListBoxEx.AddItem("Logs", "&lt;p&gt;&lt;b&gt;&lt;font color='"..String.Replace("0xFA7A18", "0x", "#", false).."'&gt;"..System.GetTime(TIME_FMT_MIL).."&lt;/font&gt; "..Type.."&lt;font color='"..String.Replace("0xFA7A18", "0x", "#", false).."'&gt;"..Text..Hint.."&lt;/font&gt;&lt;/b&gt;&lt;/p&gt;", Tooltip, "", "", 0, LBXITEM_HTML, 0, {Math.HexColorToNumber("0x404C53")}, nil, nil);
	elseif(ItemColor[1] == Math.HexColorToNumber("0x404C53"))then	
		ListBoxEx.AddItem("Logs", "&lt;p&gt;&lt;b&gt;&lt;font color='"..String.Replace("0xFA7A18", "0x", "#", false).."'&gt;"..System.GetTime(TIME_FMT_MIL).."&lt;/font&gt; "..Type.."&lt;font color='"..String.Replace("0xFA7A18", "0x", "#", false).."'&gt;"..Text..Hint.."&lt;/font&gt;&lt;/b&gt;&lt;/p&gt;", Tooltip, "", "", 0, LBXITEM_HTML, 0, {Math.HexColorToNumber("0x38454C")}, nil, nil);
	end
	
	ListBoxEx.EnsureVisible("Logs", ListBoxEx.GetTotalItems("Logs"));
end</Script>
