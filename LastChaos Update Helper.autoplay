<Script>loggedEvent1 = Watcher.GetLoggedEvent("watcher1");
loggedEvent2 = Watcher.GetLoggedEvent("watcher2");

local Status = ({});

if(loggedEvent1.Type ~= nil)then
	table.insert(Status, 1, {Event = loggedEvent1.Type, Path = loggedEvent1.Path});
end

if(loggedEvent2.Type ~= nil)then
	if(loggedEvent2.Type == "Renamed")then
		table.insert(Status, 1, {Event = loggedEvent2.Type, OldPath = loggedEvent2.OldPath, Path = loggedEvent2.Path});
	else
		table.insert(Status, 1, {Event = loggedEvent2.Type, Path = loggedEvent2.Path});
	end
end

if(#Status &gt; 0)then
	local Split = String.SplitPath(Status[1].Path);
	local Name;
	
	if(Split.Extension == "")then
		Name = (Split.Filename);
	else
		Name = (Split.Filename..Split.Extension);
	end
	
	if(Status[1].Event == "Created")then-----------------------------------------------------------------CREATED &gt;&gt; READER
		local ROW = Grid.InsertRow("Grid1", -1, true);
		
		Grid.SetCellText("Grid1", ROW, 0, string.upper(Status[1].Event), true);
		Grid.SetCellText("Grid1", ROW, 1, Name, true);
		Grid.SetCellText("Grid1", ROW, 2, Status[1].Path, true);
		
		if(ItemColorGrid == Math.HexColorToNumber(Theme.ObjectItemNormalColor))then
			ItemColorGrid = Math.HexColorToNumber(Theme.ObjectItemNormalColorB);
		else
			ItemColorGrid = Math.HexColorToNumber(Theme.ObjectItemNormalColor);
		end
		
		Grid.SetCellColors("Grid1", ROW, 0, {Background = ItemColorGrid, Text = Math.HexColorToNumber(Theme.TextColor)}, true);
		Grid.SetCellColors("Grid1", ROW, 1, {Background = ItemColorGrid, Text = Math.HexColorToNumber(Theme.TextColor)}, true);
		Grid.SetCellColors("Grid1", ROW, 2, {Background = ItemColorGrid, Text = Math.HexColorToNumber(Theme.TextColor)}, true);
	elseif(Status[1].Event == "Deleted")then-------------------------------------------------------------DELETED &gt;&gt; READY?
		for Count = 1, Grid.GetRowCount("Grid1") do
			if(String.Find(Grid.GetCellText("Grid1", Count, 2), Status[1].Path, 1, false) ~= -1)then
				Grid.SetCellText("Grid1", Count, 0, "DELETED", true);
				Grid.SetCellText("Grid1", Count, 1, "---", true);
				Grid.SetCellText("Grid1", Count, 2, "---", true);
			end
		end
	elseif(Status[1].Event == "Renamed")then--------------------------------------------------------------RENAMED &gt;&gt; READY
		local AnyFound = (false);
		
		for Count = 1, Grid.GetRowCount("Grid1") do
			if(String.Find(Grid.GetCellText("Grid1", Count, 2), Status[1].OldPath, 1, false) ~= -1)then
				Grid.SetCellText("Grid1", Count, 0, "RENAMED", true);
				
				if(Grid.GetCellText("Grid1", Count, 1) == String.SplitPath(Status[1].OldPath).Filename..String.SplitPath(Status[1].OldPath).Extension)then
					Grid.SetCellText("Grid1", Count, 1, Name, true);
				end
				
				Grid.SetCellText("Grid1", Count, 2, String.Replace(Grid.GetCellText("Grid1", Count, 2), Status[1].OldPath, Status[1].Path, true), true);
				
				AnyFound = (true);
			end
		end
		
		if(AnyFound == false)then
			local ROW = Grid.InsertRow("Grid1", -1, true);
		
			Grid.SetCellText("Grid1", ROW, 0, string.upper(Status[1].Event), true);
			Grid.SetCellText("Grid1", ROW, 1, Name, true);
			Grid.SetCellText("Grid1", ROW, 2, Status[1].Path, true);
			
			if(ItemColorGrid == Math.HexColorToNumber(Theme.ObjectItemNormalColor))then
				ItemColorGrid = Math.HexColorToNumber(Theme.ObjectItemNormalColorB);
			else
				ItemColorGrid = Math.HexColorToNumber(Theme.ObjectItemNormalColor);
			end
			
			Grid.SetCellColors("Grid1", ROW, 0, {Background = ItemColorGrid, Text = Math.HexColorToNumber(Theme.TextColor)}, true);
			Grid.SetCellColors("Grid1", ROW, 1, {Background = ItemColorGrid, Text = Math.HexColorToNumber(Theme.TextColor)}, true);
			Grid.SetCellColors("Grid1", ROW, 2, {Background = ItemColorGrid, Text = Math.HexColorToNumber(Theme.TextColor)}, true);
		end
	elseif(Status[1].Event == "Changed")then--------------------------------------------------------------CHANGED
		local AnyFound = (false);
		
		for Count = 1, Grid.GetRowCount("Grid1") do
			if(Status[1].Path == Grid.GetCellText("Grid1", Count, 2))then
				Grid.SetCellText("Grid1", Count, 0, "CHANGED", true);
				
				AnyFound = (true);
			end
		end
		
		if(AnyFound == false)then
			local ROW = Grid.InsertRow("Grid1", -1, true);
		
			Grid.SetCellText("Grid1", ROW, 0, string.upper(Status[1].Event), true);
			Grid.SetCellText("Grid1", ROW, 1, Name, true);
			Grid.SetCellText("Grid1", ROW, 2, Status[1].Path, true);
			
			if(ItemColorGrid == Math.HexColorToNumber(Theme.ObjectItemNormalColor))then
				ItemColorGrid = Math.HexColorToNumber(Theme.ObjectItemNormalColorB);
			else
				ItemColorGrid = Math.HexColorToNumber(Theme.ObjectItemNormalColor);
			end
			
			Grid.SetCellColors("Grid1", ROW, 0, {Background = ItemColorGrid, Text = Math.HexColorToNumber(Theme.TextColor)}, true);
			Grid.SetCellColors("Grid1", ROW, 1, {Background = ItemColorGrid, Text = Math.HexColorToNumber(Theme.TextColor)}, true);
			Grid.SetCellColors("Grid1", ROW, 2, {Background = ItemColorGrid, Text = Math.HexColorToNumber(Theme.TextColor)}, true);
		end
	end
	
	Grid.ExpandLastColumn("Grid1", true);

	Status = ({});
end

--loggedError = Watcher.GetLoggedError("watcher1");

--if(loggedError.Message ~= nil)then
--	ListBox.AddItem("ListBox1", loggedError.Time..", "..loggedError.Message, "");
--end</Script>
