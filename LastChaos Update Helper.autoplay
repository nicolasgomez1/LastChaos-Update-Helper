<Name>On Click</Name>
<Args/>
<Script>local SelectedProfile = ComboBox.GetSelected("CONFIG_PROFILESELECTOR");

if(SelectedProfile ~= -1)then
	local Profile = ComboBox.GetItemText("CONFIG_PROFILESELECTOR", SelectedProfile);
	
	INIFile.SetValue(SettingsFile, "Profiles", Profile, Input.GetText("CONFIG_FTP_HOST")..","..Input.GetText("CONFIG_FTP_USERNAME")..","..Crypto.BlowfishEncryptString(Input.GetText("CONFIG_FTP_PASSWORD"), "iddqd", 0)..","..Input.GetText("CONFIG_FTP_PATCHESFOLDER")..","..Input.GetText("CONFIG_MYSQL_HOST")..","..Input.GetText("CONFIG_MYSQL_USERNAME")..","..Crypto.BlowfishEncryptString(Input.GetText("CONFIG_MYSQL_PASSWORD"), "iddqd", 0)..","..Input.GetText("CONFIG_MYSQL_DBDATA"));
		
	INIFile.SetValue(SettingsFile, "General", "SelectedProfile", Profile);
	
	Dialog.TimedMessage("Profile setup", "Profile : "..ComboBox.GetItemText("CONFIG_PROFILESELECTOR", SelectedProfile).." updated.", 1200, MB_ICONINFORMATION);
	
	LoadProfiles();
end</Script>
