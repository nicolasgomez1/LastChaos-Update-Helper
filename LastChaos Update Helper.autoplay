<Event>
<Name>On Right-Click</Name>
<Args/>
<Script>while(true)do
	local ProfileName = Dialog.Input("Make new FTP Profile", "Profile Name :", "", MB_ICONNONE);
	
	if(ProfileName == "CANCEL")then
		break;
	elseif(ProfileName == "")then
		Dialog.Message("Error", "Please type a valid profile name.", MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
	else

		INIFile.SetValue(SettingsFile, "Profiles", ProfileName, Input.GetText("CONFIG_FTP_HOST")..","..Input.GetText("CONFIG_FTP_USERNAME")..","..Crypto.BlowfishEncryptString(Input.GetText("CONFIG_FTP_PASSWORD"), "iddqd", 0)..","..Input.GetText("CONFIG_FTP_PATCHESFOLDER")..","..Input.GetText("CONFIG_MYSQL_HOST")..","..Input.GetText("CONFIG_MYSQL_USERNAME")..","..Crypto.BlowfishEncryptString(Input.GetText("CONFIG_MYSQL_PASSWORD"), "iddqd", 0)..","..Input.GetText("CONFIG_MYSQL_DBDATA"));
			
		INIFile.SetValue(SettingsFile, "General", "SelectedProfile", ProfileName);
		
		Dialog.TimedMessage("Profile setup", "Profile : "..ProfileName.." added.", 1200, MB_ICONINFORMATION);
		
		LoadProfiles();
		break;
	end
end</Script>
