<HeightRatio>-1.000000</HeightRatio>
</Ratios>
<Event>
<Name>On Click</Name>
<Args/>
<Script>local HOSTNAME = Input.GetText("CONFIG_MYSQL_HOST");
local USERNAME = Input.GetText("CONFIG_MYSQL_USERNAME");
local PASSWORD = Input.GetText("CONFIG_MYSQL_PASSWORD");
local DBDATA = Input.GetText("CONFIG_MYSQL_DBDATA");

local MakeTest = (true);

if(HOSTNAME == "")then
	Input.SetProperties("CONFIG_MYSQL_HOST", {BackgroundColor = Math.HexColorToNumber(Theme.RedColor)});
	Page.SetFocus("CONFIG_MYSQL_HOST");
	MakeTest = (false);
end

if(USERNAME == "")then
	Input.SetProperties("CONFIG_MYSQL_USERNAME", {BackgroundColor = Math.HexColorToNumber(Theme.RedColor)});
	Page.SetFocus("CONFIG_MYSQL_USERNAME");
	MakeTest = (false);
end
--[[
if(PASSWORD == "")then
	Input.SetProperties("CONFIG_MYSQL_PASSWORD", {BackgroundColor = Math.HexColorToNumber(Theme.RedColor)});
	Page.SetFocus("CONFIG_MYSQL_PASSWORD");
	MakeTest = (false);
end
]]
if(DBDATA == "")then
	Input.SetProperties("CONFIG_MYSQL_DBDATA", {BackgroundColor = Math.HexColorToNumber(Theme.RedColor)});
	Page.SetFocus("CONFIG_MYSQL_DBDATA");
	MakeTest = (false);
end

if(MakeTest == true)then
	StatusDlg.Show(MB_ICONNONE, false);
	StatusDlg.ShowProgressMeter(false);
	StatusDlg.SetAutoSize(true);
	StatusDlg.SetTitle("MySQL Test Connection");
	StatusDlg.SetMessage("Please wait...");
	
	MySQLConnection, MySQLError = MySQL:connect(DBDATA, USERNAME, PASSWORD, HOSTNAME, 3306);
	if(MySQLConnection)then
		PrintLog("MySQL Connection Test -&gt; Connected to MySQL Server successfully.", "", "SUCCESS");
		
		MySQLCursor, MySQLError = MySQLConnection:execute("SELECT * FROM "..DBDATA..".t_clientversion");
		if(MySQLError ~= nil)then
			PrintLog("MySQL Connection Test -&gt; "..String.Right(MySQLError, String.Length(MySQLError)-8), "", "ERROR");
		else
			PrintLog("MySQL Connection Test -&gt; Table : 't_clientversion' was found.", "", "SUCCESS");
			MySQLCursor:close();
		end
	elseif(MySQLError ~= nil)then
		PrintLog("MySQL Connection Test -&gt; "..String.Right(MySQLError, String.Length(MySQLError)-8), "", "ERROR");
	end
	
	StatusDlg.SetStatusText("Connection Test finished.");
	StatusDlg.Hide();
end</Script>
