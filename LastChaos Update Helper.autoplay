function GetConfig(Section, Value)
	return INIFile.GetValue(SettingsFile, Section, Value);
end

WorkingFolder = GetConfig("General", "ClientFolderPath");

function FolderDeleteTreeCallback(Source, Deleted, Total)
	StatusDlg.SetMessage(("%.02f%%"):format(100*Deleted/Total));
	StatusDlg.SetStatusText("Deleting\t:\t"..Source);
	StatusDlg.SetMeterPos(100*Deleted/Total);
	
	if(Deleted == Total)then
		return false;
	else
		return true;
	end
end

function FTPDownloadCallback(nBytes, nTotal)
    if(nTotal ~= 0)then
    	local Percent = (nBytes/nTotal*100);
    	
    	StatusDlg.SetMeterPos(Percent);
    	StatusDlg.SetStatusText("Downloading\t:\t"..Percent.."%");
    end
end

function FTPUploadCallback(nBytes, nTotal)
    if(nTotal ~= 0)then
    	local Percent = (nBytes/nTotal*100);
    	
    	StatusDlg.SetMeterPos(Percent);
    	StatusDlg.SetStatusText("Uploading\t:\t"..Percent.."%");
    end
end

function ZipCallback(String, Percent, Status)
	StatusDlg.SetMessage(String);
	StatusDlg.SetStatusText("Zipping Progress\t:\t"..Percent.."%");
	StatusDlg.SetMeterPos(Percent);
end

function Stage3()
	local Continue = (true);
	local ConnectionData;
	--Tendria que verificar si ya existe una conexion mysql? hacer pruebas...
	if(FTP.IsConnected())then
		local Ret = Dialog.Message("Warning", "Do you want to close the current FTP connection?", MB_YESNO, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
		if(Ret == 6)then
			FTP.Disconnect();
			Continue = (true);
		elseif(Ret == 7)then
			Continue = (false);
		end
	else
		Continue = (true);
	end
	
	if(Continue == true)then------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------PROFILE SET
		local tblProfiles = ({});
		local SelectedProfile = (0);
	
		for _, Profile in pairs(INIFile.GetValueNames(SettingsFile, "Profiles")) do
			tblProfiles[_] = (Profile);
			if(Profile == GetConfig("General", "SelectedProfile"))then
				SelectedProfile = (_);
			end
		end
		
		while(true)do
			local Profile = Dialog.ComboBox("Stage 3", "Select a Profile", tblProfiles, tblProfiles[SelectedProfile], false, false, MB_ICONNONE);
			if(Profile == "CANCEL")then
				Continue = (false);
				break;
			else
				PrintLog("[Stage 3]\tProfile selected : '"..Profile.."'.", "", "INFO");
				ConnectionData = DelimitedToTable(GetConfig("Profiles", Profile), ",");
				Continue = (true);
				break;
			end
		end
		
		if(Continue == true)then
			StatusDlg.Show(MB_ICONNONE, false);
			StatusDlg.ShowProgressMeter(false);
			StatusDlg.SetAutoSize(true);
			StatusDlg.SetTitle("FTP and MySQL Prerequisites");
			StatusDlg.SetMessage("Trying to connect to FTP Server...");
			StatusDlg.SetStatusText("Hostname\t:\t"..ConnectionData[1].."\r\nUsername\t:\t"..ConnectionData[2].."\r\nPassword\t:\t"..ConnectionData[3]);
			
			FTP.Connect(ConnectionData[1], ConnectionData[2], Crypto.BlowfishDecryptString(ConnectionData[3], "iddqd"), "", true);-------------------------------------------------------------------------------------------------------------STEP &gt;&gt; CONNECT TO FTP
			local FTPError = Application.GetLastError();
			
			if(FTPError ~= FTP.OK)then
				StatusDlg.SetMessage("Connection to FTP Server failed...");
				StatusDlg.SetStatusText("");
				PrintLog("[Stage 3]\tFailed to connect to FTP Server, cannot continue.", "", "ERROR");
				StatusDlg.Hide();
				Continue = (false);
			else
				StatusDlg.SetMessage("Connection to FTP Server successfully established.");
				StatusDlg.SetStatusText("");
				PrintLog("[Stage 3]\tConnected to FTP Server.", "", "SUCCESS");
				---
				StatusDlg.SetMessage("Looking for Patches directory...");
				StatusDlg.SetStatusText("Directory Path\t:\t"..ConnectionData[4]);
				
				FTP.ChangeDir(ConnectionData[4]);------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------STEP &gt;&gt; CHANGE TO PATCHES FOLDER
				local WaitForError = FTP.GetLastResponse();
				if(String.Left(WaitForError, 3) == "550")then
					StatusDlg.SetMessage("Looking for Patches directory failed.");
					StatusDlg.SetStatusText("FTP Error : "..WaitForError);
					PrintLog("[Stage 3]\tFTP Error : "..WaitForError.." cannot continue.", "", "ERROR");
					StatusDlg.Hide();
					Continue = (false);
				else
					StatusDlg.SetMessage("Patches directory was found.");
					StatusDlg.SetStatusText("");
					PrintLog("[Stage 3]\tPatches directory : '"..ConnectionData[4].."' was found.", "", "SUCCESS");
					---
					StatusDlg.SetMessage("Looking for version file...");
					StatusDlg.SetStatusText("Version file Path\t:\t"..ConnectionData[4].."/version.txt");
					local FileList = FTP.ListFiles();
					if(#FileList &gt; 0)then
						local VersionExist = (false);
						
						for Count = 1, #FileList do
							if(FileList[Count].Name == "version.txt")then-------------------------------------------------------------------------------------------------------------------------------------------------------------------STEP &gt;&gt; FIND VERSION FILE
								VersionExist = (true);
								break;
							end
						end
						
						if(VersionExist == true)then
							StatusDlg.SetMessage("Version file was found.");
							StatusDlg.SetStatusText("");
							PrintLog("[Stage 3]\tVersion(version.txt) file was found.", "", "SUCCESS");
							Continue = (true);
						else
							StatusDlg.SetMessage("Looking for version file failed.");
							StatusDlg.SetStatusText("Version file(version.txt) does not exist on Patches directory.");
							PrintLog("[Stage 3]\tVersion file(version.txt) does not exist on Patches directory, cannot continue.", "", "ERROR");
							StatusDlg.Hide();
							Continue = (false);
						end
					else
						StatusDlg.SetMessage("Looking for version file failed.");
						StatusDlg.SetStatusText("Directory\t:\t'"..ConnectionData[4].."' it's empty.");
						PrintLog("[Stage 3]\tPatches directory : '"..ConnectionData[4].."' it's empty, cannot continue.", "", "ERROR");
						StatusDlg.Hide();
						Continue = (false);
					end
				end
			end
			
			if(Continue == true)then
				StatusDlg.SetMessage("Trying to connect to MySQL Server...");
				StatusDlg.SetStatusText("Hostname\t:\t"..ConnectionData[5].."\r\nUsername\t:\t"..ConnectionData[6].."\r\nPassword\t:\t"..ConnectionData[7].."\r\nDB Data\t:\t"..ConnectionData[8]);
				
				MySQLConnection, MySQLError = MySQL:connect(ConnectionData[8], ConnectionData[6], ConnectionData[7], ConnectionData[5], 3306);-----------------------------------------------------------------------------------------------STEP &gt;&gt; CONNECT TO MySQL
				if(MySQLError ~= nil)then
					StatusDlg.SetMessage("Connection to MySQL Server failed...");
					StatusDlg.SetStatusText("MySQL Error : "..String.Right(MySQLError, String.Length(MySQLError)-8));
					PrintLog("[Stage 3]\tMySQL Error : "..String.Right(MySQLError, String.Length(MySQLError)-8).." cannot continue.", "", "ERROR");
					StatusDlg.Hide();
					Continue = (false);
				else
					StatusDlg.SetMessage("Connection to MySQL Server successfully established.");
					StatusDlg.SetStatusText("Connected to Database\t:\t'"..ConnectionData[8].."'.");
					PrintLog("[Stage 3]\tConnected to MySQL Database : '"..ConnectionData[8].."'.", "", "SUCCESS");
					---
					StatusDlg.SetMessage("Looking for t_clientversion table...");
					StatusDlg.SetStatusText("Database\t:\t"..ConnectionData[8].."\tTable\t:\tt_clientversion");
					
					MySQLCursor, MySQLError = MySQLConnection:execute("SELECT * FROM "..ConnectionData[8]..".t_clientversion");---------------------------------------------------------------------------------------------------------------STEP &gt;&gt; FIND t_clientversion Table
					if(MySQLError ~= nil)then
						StatusDlg.SetMessage("Table\t:\t't_clientversion' not found.");
						StatusDlg.SetStatusText("MySQL Error : "..String.Right(MySQLError, String.Length(MySQLError)-8));
						PrintLog("MySQL Error : "..String.Right(MySQLError, String.Length(MySQLError)-8).." cannot continue.", "", "ERROR");
						StatusDlg.Hide();
						Continue = (false);
					else
						StatusDlg.SetMessage("Version file was found.");
						StatusDlg.SetStatusText("");
						PrintLog("[Stage 3]\tTable : 't_clientversion' was found.", "", "SUCCESS");
						Continue = (true);
						PrintLog("[Stage 3]\tAll prerequisites found.", "", "SUCCESS");
					end
				end
				
				if(Continue == true)then
					PrintLog("[Stage 3]\tDownloading version file...", "", "INFO");
					StatusDlg.SetTitle("Gettin version from FTP Server");
					StatusDlg.SetMessage("Downloading version.txt from FTP Server...");
					StatusDlg.ShowProgressMeter(true);
					StatusDlg.SetMeterRange(0, 100);
					FTP.Download("version.txt", _SourceFolder.."\\version.txt.TEMP", FTPDownloadCallback);
					local FTPError = Application.GetLastError();
					
					StatusDlg.Hide();
											
					if(FTPError ~= FTP.OK)then
						PrintLog("[Stage 3]\tFTP Error : ".._tblErrorMessages[FTPError].." cannot continue.", "", "ERROR");
						Continue = (false);
					else
						PrintLog("[Stage 3]\tVersion file downloaded successfully.", "", "SUCCESS");
						local Version = tonumber(TextFile.ReadToString(_SourceFolder.."\\version.txt.TEMP"));
						
						if(Version == 0)then
							Version = (1);
						end
						
						local UseCustomVersion = (nil);
						
						if(Version == "" or type(Version) ~= "number")then
							while(true)do
								local ManualVersion = Dialog.Input("Version file it's empty", "Please define a version for the update", "1", MB_ICONSTOP);
								if(ManualVersion == "CANCEL")then
									local Ret = Dialog.Message("Warning", "Are you sure you want to cancel?", MB_YESNO, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
									if(Ret == 6)then
										Dialog.TimedMessage("Fuck you", "Well dude, u mom clean my socks...", 2000, MB_ICONNONE);
										Continue = (false);
										break;
									end
								elseif(type(tonumber(ManualVersion)) ~= "number")then
									Dialog.Message("Error", "Version not valid.\r\nplease make sure to enter a valid numeric version.", MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
								elseif(type(tonumber(ManualVersion)) == "number")then
									Dialog.TimedMessage("New Version", "'"..ManualVersion.."' Has been defined as next update version.", 1200, MB_ICONINFORMATION);
									UseCustomVersion = (ManualVersion);
									Continue = (true);
									break;
								end	
							end
						else
							Continue = (true);
						end
						
						if(Continue == true)then
							if(UseCustomVersion == nil)then
								Version = (Version+1);
							else
								Version = (UseCustomVersion);
							end
							
							local ZipPath = (_SourceFolder.."\\"..String.Replace(System.GetDate(DATE_FMT_ISO), "-", "_", true).."-"..String.Replace(System.GetTime(TIME_FMT_MIL), ":", "_", true).."-"..Version..".zip");
							
							StatusDlg.Show(MB_ICONNONE, false);
							StatusDlg.ShowProgressMeter(true);
							StatusDlg.SetAutoSize(true);
							StatusDlg.SetMeterRange(0, 100);
							StatusDlg.SetTitle("Zipping : "..String.Replace(System.GetDate(DATE_FMT_ISO), "-", "_", true).."-"..String.Replace(System.GetTime(TIME_FMT_MIL), ":", "_", true).."-"..Version..".zip");
							StatusDlg.SetMessage("Please Wait...");
							PrintLog("[Stage 3]\tCreating patch file...", "", "INFO");

							Zip.Add(ZipPath, { _SourceFolder.."\\temp\\*.*"}, true, "", 5, ZipCallback, true);
							local ZIPError = Application.GetLastError();
							
							StatusDlg.Hide();
							
							if(ZIPError == 0)then
								PrintLog("[Stage 3]\tPatch file was created successfully.", "", "SUCCESS");
								---
								PrintLog("[Stage 3]\tCreating new version file...", "", "INFO");
								
								TextFile.WriteFromString(_SourceFolder.."\\version.txt.NEW", Version, false);
								local TXTError = Application.GetLastError();
								if(TXTError == 0)then
									PrintLog("[Stage 3]\tVersion file was created.", "", "SUCCESS");
									---
									PrintLog("[Stage 3]\tTrying to upload patch files to server...", "", "INFO");
									StatusDlg.SetTitle("Uploading patch files to server");
									StatusDlg.SetMessage("Uploading\t:\t"..Version..".zip and version.txt");
									StatusDlg.ShowProgressMeter(true);
									StatusDlg.SetMeterRange(0, 100);
									
									FTP.Upload(ZipPath, Version..".zip", FTPUploadCallback);
									local FTPError = Application.GetLastError();
									if(FTPError == FTP.OK)then
										PrintLog("[Stage 3]\tPatch file was uploaded to server successfully.", "", "SUCCESS");
										
										FTP.Upload(_SourceFolder.."\\version.txt.NEW", "version.txt", FTPUploadCallback);
										local FTPError = Application.GetLastError();
										if(FTPError == FTP.OK)then
											PrintLog("[Stage 3]\tVersion file was uploaded to server successfully.", "", "SUCCESS");
											MySQLCursor, MySQLError = MySQLConnection:execute("UPDATE "..ConnectionData[8]..".t_clientversion SET a_min = '"..Version.."' , a_max = '"..Version+1 .."'");
											if(MySQLError ~= nil)then
												PrintLog("[Stage 3]\tMySQL Error : "..String.Right(MySQLError, String.Length(MySQLError)-8).." cannot continue.", "", "ERROR");
												---
												PrintLog("[Stage 3]\tBecause the table could not be updated in the database, the changes will be reverted on the ftp server...", "", "WARNING");
												FTP.Delete(Version..".zip");
												---
												StatusDlg.SetTitle("Reverting version file");
												StatusDlg.SetMessage("Uploading Old\t:\tversion.txt");
												StatusDlg.ShowProgressMeter(true);
												StatusDlg.SetMeterRange(0, 100);
												
												FTP.Upload(_SourceFolder.."\\version.txt.TEMP", "version.txt", FTPUploadCallback);
												local FTPError = Application.GetLastError();
												
												StatusDlg.Hide();
												
												if(FTPError == FTP.OK)then
													PrintLog("[Stage 3]\tVersion file reverted successfully.", "", "SUCCESS");
													--poner la funcion clean
												else
													PrintLog("[Stage 3]\tFTP Error : ".._tblErrorMessages[FTPError].." FUCK IT BYE BYE.", "", "ERROR");
												end
												--Continue = (false);
											else
												PrintLog("[Stage 3]\tTable : 't_clientversion' was successfully updated.", "", "SUCCESS");
												MySQLConnection:close();
												FTP.Disconnect();
												PrintLog("[Stage 3]\tCompleted, New patch it's online and ready!.", "", "SUCCESS");
												---
												tblMenu[5].Enabled = (false);
												tblMenu[7].Enabled = (false);
												Application.SetMenu(tblMenu);
												---
												PrintLog("Cleaning residual files..", "", "INFO");
												
												File.Delete(_SourceFolder.."\\version.txt.TEMP", false, false, false, nil);
												
												File.Delete(_SourceFolder.."\\version.txt.NEW", false, false, false, nil);
												
												StatusDlg.Show(MB_ICONNONE, false);
												StatusDlg.SetTitle("Cleaning temporary folder...");
												StatusDlg.SetMessage("Please Wait...");
												StatusDlg.ShowProgressMeter(true);
												StatusDlg.SetMeterRange(0, 100);
												
												--Folder.DeleteTree(TempFolder, FolderDeleteTreeCallback);
												Folder.Delete(TempFolder);
												
												StatusDlg.Hide();
											end
										else
											PrintLog("[Stage 3]\tFTP Error : ".._tblErrorMessages[FTPError].." cannot continue.", "", "ERROR");
											---
											PrintLog("[Stage 3]\tBecause the Version file could not be uploaded to the server, the previously uploaded Patch file will be deleted.", "", "WARNING");
											FTP.Delete(Version..".zip");
											--poner la funcion clean
											StatusDlg.Hide();
											--Continue = (false);
										end
									else
										PrintLog("[Stage 3]\tFTP Error : ".._tblErrorMessages[FTPError].." cannot continue.", "", "ERROR");
										StatusDlg.Hide();
										--Continue = (false);
									end
								else
									PrintLog(_tblErrorMessages[TXTError].." cannot continue.", "", "ERROR");
									--Continue = (false);
								end
							else
								PrintLog("[Stage 3]\tZIP Error : ".._tblErrorMessages[ZIPError].." cannot continue.", "", "ERROR");
								--Continue = (false);
							end
						end
					end
				end
			end
		end
	end
end

function Stage2()
	--[[NOTES
		la ruta se forma con doble \, sin enbargo funciona perfectamente.
	]]
	if(Grid.GetRowCount("Grid1") &gt; 0)then
		--PrintLog("[Stage 2]\tCleaning temporary folder...", "", "INFO");
		StatusDlg.SetTitle("Cleaning temporary folder...");
		StatusDlg.SetMessage("Please Wait...");
		StatusDlg.ShowProgressMeter(true);
		StatusDlg.SetMeterRange(0, 100);

		--Folder.DeleteTree(TempFolder, FolderDeleteTreeCallback);
		Folder.Delete(TempFolder);
		
		StatusDlg.Hide();
		
		PrintLog("[Stage 2]\tPreparing temporary folder...", "", "INFO");
		Folder.Create(TempFolder);
		PrintLog("[Stage 2]\tStarted.", "", "INFO");
		
		for Count = 1, Grid.GetRowCount("Grid1") do
			local Path = String.Replace(Grid.GetCellText("Grid1", Count, 2), WorkingFolder, "", true);
			local Split = String.SplitPath(_SourceFolder.."\\temp\\"..Path);
	
			if(Split.Extension == "")then--FOLDER
				local BuildingPath = (TempFolder);
				for _, FolderName in pairs(DelimitedToTable(Path, "\\")) do
					PrintLog("[Stage 2]\tMaking folder : '"..BuildingPath.."\\"..FolderName.."'...", "", "INFO");
					Folder.Create(BuildingPath.."\\"..FolderName);
					BuildingPath = (BuildingPath.."\\"..FolderName);
				end
			else---------------------------FILE
				if(Folder.DoesExist(Split.Drive..Split.Folder))then
					File.Copy(Grid.GetCellText("Grid1", Count, 2), TempFolder.."\\"..Path, true, true, false, true, nil);
				else
					local BuildingPath = (TempFolder);
					local Split2 = String.SplitPath(Path);
					
					for _, FolderName in pairs(DelimitedToTable(String.Replace(Path, "\\"..Split2.Filename..Split2.Extension, "", true), "\\")) do
						PrintLog("[Stage 2]\tMaking folder : '"..BuildingPath.."\\"..FolderName.."'...", "", "INFO");
						Folder.Create(BuildingPath.."\\"..FolderName);
						BuildingPath = (BuildingPath.."\\"..FolderName);
					end
					
					if(Folder.DoesExist(Split.Drive..Split.Folder))then
						PrintLog("[Stage 2]\tCopying file : '"..Grid.GetCellText("Grid1", Count, 2).."' &gt;&gt; '"..TempFolder.."\\"..Path.."'...", "", "INFO");
						File.Copy(Grid.GetCellText("Grid1", Count, 2), TempFolder.."\\"..Path, true, true, false, true, nil);
					end
				end
			end
		end
		
		PrintLog("[Stage 2]\tCompleted.", "", "INFO");
		local Ret = Dialog.Message("Stage 2 Completed", "Do you want to continue to Stage 3?", MB_YESNO, MB_ICONINFORMATION, MB_DEFBUTTON1);
		if(Ret == 6)then
			Stage3();
		elseif(Ret == 7)then
			tblMenu[5].Enabled = (true);
			tblMenu[7].Enabled = (true);
			Application.SetMenu(tblMenu);
		end
	else
		PrintLog("[Stage 2]\tGrid it's empty.", "", "ERROR");
	end
end

--[[
	Desc	:	Print logs on console.
	Arg 1	:	Text(string)
	Arg 2	:	Tooltip(string)
	Arg 3	:	Log type(String) INFO / ERROR / SUCCESS / WARNING
]]
function PrintLog(Text, Tooltip, Type)
	local Hint = ("");
	
	if(Type == "INFO")then			Type = ("&lt;font color='"..String.Replace(Theme.BlueColor, "0x", "#", false).."'&gt;INFO\t\t&lt;/font&gt;");
	elseif(Type == "ERROR")then		Type = ("&lt;font color='"..String.Replace(Theme.RedColor, "0x", "#", false).."'&gt;ERROR\t&lt;/font&gt;");
	elseif(Type == "SUCCESS")then	Type = ("&lt;font color='"..String.Replace(Theme.GreenColor, "0x", "#", false).."'&gt;SUCCESS\t&lt;/font&gt;");
	elseif(Type == "WARNING")then	Type = ("&lt;font color='"..String.Replace(Theme.YellowColor, "0x", "#", false).."'&gt;WARNING\t&lt;/font&gt;");	end
	
	if(Tooltip ~= "")then	Hint = (" (*)");	end
	
	local ItemColor = ListBoxEx.GetItemColor("Logs", ListBoxEx.GetTotalItems("Logs"), LBXISTATE_NORMAL);
