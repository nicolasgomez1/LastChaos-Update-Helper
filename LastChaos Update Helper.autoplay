<Script>local SelectedProfile = ComboBox.GetSelected("CONFIG_FTP_PROFILESELECTOR");

if(SelectedProfile ~= -1)then
	local Profile = ComboBox.GetItemText("CONFIG_FTP_PROFILESELECTOR", SelectedProfile);
	
	INIFile.SetValue(SettingsFile, "FTP_Profiles", Profile, Input.GetText("CONFIG_FTP_HOST")..","..Input.GetText("CONFIG_FTP_USERNAME")..","..Crypto.BlowfishEncryptString(Input.GetText("CONFIG_FTP_PASSWORD"), "iddqd", 0)..","..Input.GetText("CONFIG_FTP_LAUNCHERFOLDER")..","..Input.GetText("CONFIG_FTP_PATCHESFOLDER"));
		
	INIFile.SetValue(SettingsFile, "General", "SelectedProfile", Profile);
	
	Dialog.TimedMessage("FTP Setup Profile", "Profile : "..ComboBox.GetItemText("CONFIG_FTP_PROFILESELECTOR", SelectedProfile).." updated.", 1200, MB_ICONINFORMATION);
	
	LoadProfiles();
end</Script>
